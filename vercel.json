{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "resources/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/resources/(.*)",
      "dest": "/resources/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 